    "@babel/plugin-transform-react-jsx-source": "^7.8",
    "@babel/preset-env": "^7.8",
    "@babel/preset-typescript": "^7.8",
    "@denysvuika/preact-translate": "^0.2.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
